{
  "name": "nursing-home-volunteer",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src",
    "format": "prettier --write .",
    "prepare": "cd ../ && husky install ./frontend/.husky",
    "update:dependencies": "npx npm-check-updates -i"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix",
    "*.{js,json,ts,html,scss,vue,md}": "prettier --write"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.0",
    "marked": "^4.0.1",
    "pinia": "^2.0.3",
    "sanitize-html": "^2.5.3",
    "striptags": "^3.2.0",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.6",
    "@types/marked": "^4.0.0",
    "@types/sanitize-html": "^2.5.0",
    "@types/striptags": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-vue": "^1.9.4",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "vite-plugin-eslint": "^1.3.0",
    "vue-tsc": "^0.29.3"
  }
}
